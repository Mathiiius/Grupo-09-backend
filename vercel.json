{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/movies/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/reviews/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/genres/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/movie_genres/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}